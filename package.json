{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm install && cd backend && npm install",
    "backend-dev": "cd backend && nodemon ./src/app.ts",
    "openapi-gen": "openapi --input http://localhost:3000/docs/swagger.json --output ./src/api --client axios",
    "dev": "vite --host",
    "build": "tsc && openapi-gen && vite build",
    "backend-build-start": "cd backend && tsc && node ./dist/app.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@types/tailwindcss": "^3.1.0",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^5.3.2",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "express-jwt-authz": "^2.4.1",
    "express-openid-connect": "^2.17.1",
    "express-session": "^1.18.0",
    "framer-motion": "^10.16.16",
    "i18next": "^23.7.11",
    "jwks-rsa": "^3.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.21.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express-session": "^1.17.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.0.2",
    "openapi-typescript-codegen": "^0.27.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
