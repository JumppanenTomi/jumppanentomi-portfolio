{
  "source": {
    "include": [
      "./src",
      "./README.md"
    ],
    "includePattern": ".+\\.(js|ts)(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "node_modules/better-docs/typescript"
  ],
  "opts": {
    "destination": "./src/docs/",
    "encoding": "utf8",
    "recurse": true,
    "template": "node_modules/better-docs",
    "readme": "./README.md"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}